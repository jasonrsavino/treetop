{
  "page": {
    "header": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--header"]
            }
          },
          "content": {
            "include()": {
              "pattern": "organisms-system-branding",
              "with": {
                "attributes": {
                  "Attribute()": {
                    "class": ["block","block--branding"]
                  }
                }
              }
            }
          }
        }
      }
    },
    "primary_menu":{
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--primary-menu"]
            }
          },
          "content": {
            "include()": {
              "pattern": "organisms-block",
              "with": {
                "label": "Primary Menu Block",
                "attributes": {
                  "Attribute()": {
                    "class": ["block","block--system-menu"]
                  }
                },
                "content": ""
              }
            }
          }
        }
      }
    },
    "secondary_menu":{
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--secondary-menu"]
            }
          },
          "content": {
            "include()": {
              "pattern": "organisms-block",
              "with": {
                "label": "Secondary Menu Block",
                "attributes": {
                  "Attribute()": {
                    "class": ["block","block--secondary-menu"]
                  }
                },
                "content": ""
              }
            }
          }
        }
      }
    },
    "breadcrumb":{
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--breadcrumb"]
            }
          },
          "content": {
            "include()": {
              "pattern": "organisms-block",
              "with": {
                "label": "Breadcumb Menu Block",
                "attributes": {
                  "Attribute()": {
                    "class": ["block","block--breadcrumb"]
                  }
                },
                "content": ""
              },
              "only": true
            }
          }
        }
      }
    },
    "highlighted":{
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--highlighted"]
            }
          },
          "content": {
            "include()": {
              "pattern": "organisms-block",
              "with": {
                "label": "Highlighted Block",
                "attributes": {
                  "Attribute()": {
                    "class": ["block","block--block"]
                  }
                },
                "content": ""
              },
              "only": true
            }
          }
        }
      }
    },
    "help": {
        "include()": {
          "pattern": "organisms-region",
          "with": {
            "attributes": {
              "Attribute()": {
                "class": ["region--help"]
              }
            },
            "content": {
              "join()": [
                {
                  "include()": {
                    "pattern": "organisms-block",
                    "with": {
                      "label": "Help Block",
                      "attributes": {
                        "Attribute()": {
                          "class": ["block","block--help"]
                        }
                      },
                    "content": "Help messages display here."
                    },
                    "only": true
                  }
                },
                {
                  "include()": {
                    "pattern": "organisms-block",
                    "with": {
                      "label": "Status Message Block",
                      "attributes": {
                        "Attribute()": {
                          "class": ["block","block--status-messages"]
                        }
                      },
                    "content": "Status messages appear here."
                    },
                    "only": true
                  }
                }
              ]
            }
          }
        }
    },
    "content": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--content"]
            }
          },
          "content": {
            "join()": [
              {
                "include()": {
                  "pattern": "organisms-block",
                  "with": {
                    "attributes": {
                      "Attribute()": {
                        "class": ["block","block--page-title"]
                      }
                    },
                    "content": "molecules-page-title"
                  },
                  "only": true
                }
              },
               {
                "include()": {
                  "pattern": "organisms-block",
                  "with": {
                    "attributes": {
                      "Attribute()": {
                        "class": ["block","block--content"]
                      }
                    },
                    "content": "organisms-article"
                  },
                  "only": true
                }
              }
            ]
          }
        }
      }
    },
    "sidebar_first":{
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--sidebar-first"]
            }
          },
          "content": {
            "include()": {
              "pattern": "organisms-block",
              "with": {
                "label": "Sidebar First Block",
                "attributes": {
                  "Attribute()": {
                    "class": ["block","block--block"]
                  }
                }
              }
            }
          }
        }
      }
    },
    "sidebar_second": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--sidebar-second"]
            }
          },
          "content": {
            "include()": {
              "pattern": "organisms-block",
              "with": {
                "label": "Sidebar Second Block",
                "attributes": {
                  "Attribute()": {
                    "class": ["block","block--block"]
                  }
                }
              }
            }
          }
        }
      }
    },
    "footer": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "attributes": {
            "Attribute()": {
              "class": ["region--footer"]
            }
          },
          "content": {
            "include()": {
              "pattern": "organisms-block",
              "with": {
                "label": "Footer Block",
                "attributes": {
                  "Attribute()": {
                    "class": ["block","block--block"]
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
