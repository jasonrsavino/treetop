{
  "page": {
    "breadcrumb": "molecules-breadcrumb",
    "header": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "region": "header",
          "attributes": {
            "Attribute()": {
              "class": []
            }
          },
          "content": {
            "include()": {
              "pattern": "molecules-branding-block"
            }
          }
        },
        "only": true
      }
    },
    "primary_menu": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "region": "primary-menu",
          "attributes": {
            "Attribute()": {
              "class": []
            }
          },
          "content": {
            "include()": {
              "pattern": "molecules-menu-block"
            }
          }
        },
        "only": true
      }
    },
    "secondary_menu": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "region": "secondary-menu",
          "attributes": {
            "Attribute()": {
              "class": []
            }
          },
          "content": {
            "include()": {
              "pattern": "molecules-menu-block",
              "only": true,
              "with": {
                "derivative_plugin_id": "account",
                "attributes": {
                  "Attribute()": {
                    "id": ["block-account-menu"]
                  }
                },
                "configuration": {
                  "label_display": false
                },
                "content": {
                  "include()": {
                    "pattern": "molecules-menu",
                    "only": true,
                    "with": {
                      "menu_name": "account-menu",
                      "attributes": {
                        "Attribute()": {
                          "class": ["menu"]
                        }
                      },
                      "items": [
                        {
                          "attributes": {
                            "Attribute()": {
                              "class": ["menu-item"]
                            }
                          },
                          "title": "My account",
                          "url": "#"
                        },
                        {
                          "attributes": {
                            "Attribute()": {
                              "class": ["menu-item"]
                            }
                          },
                          "title": "Log out",
                          "url": "#"
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "only": true
      }
    },
    "content": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "region": "content",
          "attributes": {
            "Attribute()": {
              "class": []
            }
          },
          "content": {
            "join()": [
              {
                "include()": {
                  "pattern": "atoms-page-title",
                  "with": {
                    "title": "molecules-field--node--title"
                  }
                }
              },
              "organisms-node"
            ]
          }
        },
        "only": true
      }
    },
    "sidebar_first": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "region": "sidebar_first",
          "attributes": {
            "Attribute()": {
              "class": []
            }
          },
          "content": {
            "include()": {
              "pattern": "molecules-menu-block",
              "with": {
                "derivative_plugin_id": "tools",
                "attributes": {
                  "Attribute()": {
                    "id": ["block-tools"]
                  }
                },
                "configuration": {
                  "label": "Tools"
                },
                "content": {
                  "include()": {
                    "pattern": "molecules-menu",
                    "only": true,
                    "with": {
                      "menu_name": "tools",
                      "attributes": {
                        "Attribute()": {
                          "class": ["menu"]
                        }
                      },
                      "items": [
                        {
                          "attributes": {
                            "Attribute()": {
                              "class": ["menu-item"]
                            }
                          },
                          "title": "Add content",
                          "url": "#"
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "only": true
      }
    },
    "sidebar_second": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "region": "sidebar_second",
          "attributes": {
            "Attribute()": {
              "class": []
            }
          },
          "content": {
            "include()": {
              "pattern": "molecules-block",
              "with": {
                "label": "Recent Content",
                "configuration": {
                  "provider": "views"
                },
                "plugin_id": "views-blockcontent-recent-block-1",
                "attributes": {
                  "Attribute()": {
                    "id": ["block-views-block-content-recent-block-1"]
                  }
                },
                "title_attributes": {
                  "Attribute()": {
                    "class": [],
                    "id": []
                  }
                },
                "content": "molecules-links"
              }
            }
          }
        },
        "only": true
      }
    },
    "footer": {
      "include()": {
        "pattern": "organisms-region",
        "with": {
          "region": "footer",
          "attributes": {
            "Attribute()": {
              "class": []
            }
          },
          "content": {
            "include()": {
              "pattern": "molecules-block",
              "with": {
                "label": false,
                "configuration": {
                  "provider": "system"
                },
                "plugin_id": "block-system-powered-by-block",
                "attributes": {
                  "Attribute()": {
                    "id": ["block-poweredbydrupal"]
                  }
                },
                "content": "<span>Powered by <a href=\"https://www.drupal.org\" target=\"_blank\">Drupal</a></span>"
              }
            }
          }
        },
        "only": true
      }
    }
  }
}
