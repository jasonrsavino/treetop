{
"output_image_tag": false,
"img_element": {
  "include()": {
    "pattern": "molecules-image",
    "with": {
      "attributes": {
        "Attribute()": {
          "src": "../../images/fpo_16x9.png"
        }
      }
    },
    "only": true
  }
},
"sources": [
  {
    "Attribute()": {
      "media": ["(max-width: 960px)"],
      "srcset": ["../../images/fpo_4x3.png"]
    }
  },
  {
    "Attribute()": {
      "media": ["(min-width: 1280px)"],
      "srcset": ["../../images/fpo_16x9.png"]
    }
  }
]
}
