{
  "attributes": {
    "Attribute()": {
      "class": ["field--type-string","field--name-title","field--widget-string-textfield"],
      "id": "edit-title-wrapper"
    }
  },
  "has_parent": true,
  "children": {
      "include()": {
        "pattern": "molecules-form-element",
        "with": {
          "title_display": "before",
          "description_display": "invisible",
          "attributes": {
            "Attribute()": {
              "id": "edit-title-wrapper"
            }
          },
         "label_display": "before",
          "label": {
            "include()": {
              "pattern": "atoms-form-element-label",
              "with": {
                "title": "Title",
                "title_display": "before",
                "attributes": {
                  "Attribute()": {
                    "for": "edit-textfield"
                  }
                }
              }
            }
          }
        }
      }
    }
}
